<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

             <!-- Welcome File Configuration -->
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>events.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>EventServlet</servlet-name>
        <servlet-class>com.collegefest.servlet.EventServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventServlet</servlet-name>
        <url-pattern>/event</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ParticipantServlet</servlet-name>
        <servlet-class>com.collegefest.servlet.ParticipantServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ParticipantServlet</servlet-name>
        <url-pattern>/participant</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VolunteerServlet</servlet-name>
        <servlet-class>com.collegefest.servlet.VolunteerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>VolunteerServlet</servlet-name>
        <url-pattern>/volunteer</url-pattern>
    </servlet-mapping>

</web-app>

